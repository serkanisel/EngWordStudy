@model List<EWS.Model.EWSListWord>

@foreach (var item in Model)
{
    <div class="col-lg-4 col-md-3 col-sm-12 p-0" id="divListWord_@item.UN">
        <div class="m-1 p-3 rounded bg-light">
            <h5 class="font-italic"><a href="javascript:void(0)" onclick="getWordDetails('@item.EWSWord.UN')">@item.EWSWord.WordBody</a></h5>
            <p class="mb-2">@item.EWSWord.Description</p>
            <a href="javascript:void(0)" class="btn btn-warning" onclick="ListedenCikar('@item.EWSWord.UN')"><i class="fa fa-minus"></i> Listeden Çıkar</a>
        </div>
    </div>
}


