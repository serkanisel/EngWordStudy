@model List<EWS.Model.EWSSampleSentence>

@{
    int a = 0;
    foreach (var item in Model)
    {
        a++;
        <div class="row">
            <div class="col-12">
                <h5 class="font-italic"><span class="badge badge-light">@a.ToString()</span> @item.Sentence</h5>
                <p class="mb-2">@item.SentenceMean</p>
            </div>
        </div>

    }
}